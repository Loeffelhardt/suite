<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RpcCall">
        <property name="method" type="string"/>
        <property name="arguments" type="RpcArgument[]"/>
    </transfer>
    <transfer name="RpcArgument">
        <property name="type" type="string"/>
        <property name="value" type="object"/>
    </transfer>
    <transfer name="RpcTypeBoolean">
        <property name="value" type="bool"/>
    </transfer>
    <transfer name="RpcTypeString">
        <property name="value" type="string"/>
    </transfer>
    <transfer name="RpcTypeInteger">
        <property name="value" type="int"/>
    </transfer>
    <transfer name="RpcTypeFloat">
        <property name="value" type="int"/>
    </transfer>
    <transfer name="RpcTypeArray">
        <property name="value" type="array"/>
    </transfer>
    <transfer name="RpcTypeNull">
    </transfer>
</transfers>
